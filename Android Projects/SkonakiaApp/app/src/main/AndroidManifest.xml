<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="matrix.skonakigr.kostas.skonakiaapp" >

    <application
        android:allowBackup="true"
        android:icon="@mipmap/launch_icon"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name="matrix.skonakigr.kostas.skonakiaapp.MainScreen"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="matrix.skonakigr.kostas.skonakiaapp.Tropoi"
            android:parentActivityName="matrix.skonakigr.kostas.skonakiaapp.MainScreen"
            android:label="@string/title_activity_next" >
        </activity>
        <activity
            android:parentActivityName="matrix.skonakigr.kostas.skonakiaapp.Tropoi"
            android:name="matrix.skonakigr.kostas.skonakiaapp.TroposItem" >
        </activity>
    </application>

</manifest>
